<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="maps.googleapis.com-script" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="maps.googleapis.com-connect" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="maps.googleapis.com-frame" type="host">*.googleapis.com</value>
                <value id="google-maps-frame" type="host">*.google.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="maps.gstatic" type="host">maps.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
