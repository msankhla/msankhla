<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table comment="blueacorn_content_version Table" engine="innodb" name="blueacorn_content_version" resource="default">
        <column comment="Entity Id" identity="true" name="content_id" nullable="false" padding="6" unsigned="false" xsi:type="smallint"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="content_id"/>
        </constraint>
        <column name="type" nullable="true" length="255" xsi:type="varchar"/>
        <column name="identifier" nullable="true" length="255" xsi:type="varchar"/>
        <column name="version" nullable="true" length="255" xsi:type="varchar"/>
    </table>
</schema>
